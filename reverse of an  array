#include <iostream>

void printArrayReverse(int arr[], int size)
{
    if (size == 0) {
        return;
        cout << arr[size - 1] << " ";

    printArrayReverse(arr, size - 1); 

int main()
{
    int n;
    cin >> n;

    int arr[n];
   
    for (int i = 0; i < n; i++) {
      cin >> arr[i];
    }

 
    printArrayReverse(arr, n);

    return 0;
}
